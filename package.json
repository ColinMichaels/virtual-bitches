{
  "name": "biscuits-game",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "brand:audit": "node scripts/brand-audit.mjs",
    "brand:rewrite": "node scripts/brand-rewrite.mjs",
    "test:brand-rewrite": "node scripts/brand-rewrite.test.mjs",
    "updates:generate": "node scripts/generate-updates-from-git.mjs",
    "copy:assets": "node scripts/copy-assets.mjs",
    "copy:assets:sync": "node scripts/copy-assets.mjs --sync-themes",
    "dev": "npm run updates:generate && npm run copy:assets && vite",
    "dev:full": "node scripts/dev-full.mjs",
    "dev:dev": "npm run updates:generate && npm run copy:assets && vite --mode development",
    "build": "npm run updates:generate && npm run copy:assets && tsc && vite build",
    "build:dev": "npm run updates:generate && npm run copy:assets && tsc && vite build --mode development",
    "build:prod": "npm run updates:generate && npm run copy:assets && tsc && vite build --mode production",
    "assets:optimize": "node scripts/optimize-public-images.mjs",
    "preview": "vite preview",
    "test": "tsx src/engine/rules.test.ts",
    "test:camera": "tsx src/services/cameraService.test.ts",
    "test:camera-effects": "tsx src/services/cameraEffects.test.ts",
    "test:backend-api": "tsx src/services/backendApi.test.ts",
    "test:chaos-camera": "tsx src/chaos/cameraAttackExecutor.test.ts",
    "test:chaos-execution": "tsx src/chaos/upgrades/executionProfile.test.ts",
    "test:chaos-upgrades": "tsx src/chaos/upgrades/progressionService.test.ts",
    "test:controls": "tsx src/services/controlInversion.test.ts",
    "test:i18n": "tsx src/i18n/i18n.test.ts",
    "test:network": "tsx src/multiplayer/networkService.test.ts",
    "test:session-service": "tsx src/multiplayer/sessionService.test.ts",
    "test:session-expiry-flow": "tsx src/multiplayer/sessionExpiryFlow.test.ts",
    "test:bot-engine": "npm --prefix api run test:bot-engine",
    "test:e2e:api": "node api/e2e/smoke.mjs",
    "test:e2e:api:local": "node api/e2e/run-local.mjs",
    "api:migrate:firestore": "node api/scripts/migrate-file-store-to-firestore.mjs",
    "api:migrate:firestore:verify": "node api/scripts/migrate-file-store-to-firestore.mjs --verify-only",
    "api:dev": "node api/server.mjs",
    "cdn:verify": "node scripts/check-cdn-assets.mjs",
    "cdn:upload:firebase": "npm run updates:generate && npm run copy:assets && node scripts/upload-assets-to-firebase-storage.mjs",
    "firebase:login": "npx firebase-tools login",
    "firebase:use:prod": "npx firebase-tools use prod",
    "firebase:emulators": "npx firebase-tools emulators:start",
    "firebase:deploy:hosting": "npm run build:prod && npx firebase-tools deploy --only hosting",
    "firebase:deploy:firestore": "npx firebase-tools deploy --only firestore:rules,firestore:indexes",
    "firebase:deploy": "npm run build:prod && npx firebase-tools deploy --only hosting,firestore",
    "cloudrun:deploy:api": "node scripts/deploy-cloudrun-api.mjs",
    "deploy": "npm run build && npx gh-pages -d dist"
  },
  "dependencies": {
    "@babylonjs/core": "^7.0.0",
    "@babylonjs/loaders": "^7.0.0",
    "firebase": "^12.9.0",
    "marked": "^17.0.3"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0"
  }
}
